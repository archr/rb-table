{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "curly": 0,
    "quotes": [2, "single"],
    "new-cap": 0,
    "no-underscore-dangle": 0,
    "no-wrap-func": 0
  }
}
